task input_task; begin
    repeat( ({$random(SEED)} % 4 + 2) ) @(negedge clk);
    reset_map;
    gen_valid_path;
    gen_map;
    // show_map_with_valid_path;

    for(j=0 ; j<NUM_CYCLE ; j=j+1) begin
        if(j==0) init = start_pos;
        else             init = 'dx;
        in_valid = 1;
        in0      = map[0][j];
        in1      = map[1][j];
        in2      = map[2][j];
        in3      = map[3][j];
        @(negedge clk);
    end
    in_valid = 0;
    in0      = 'dx;
    in1      = 'dx;
    in2      = 'dx;
    in3      = 'dx;
end endtask

